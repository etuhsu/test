io.open()

for(i=1;9){
    for(j=1;i){
    	io.stdout.write(  string.format("%d*%d=%2d  ",i,j,i*j) )
    }
    
    io.print('\n');
}

execute("pause") //按任意键继续
io.close();//关闭控制台
