import console;
import crypt.bin;
import crypt.hmac;
 
key = '\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b\x0b';
data =  "Hi There"; 
  
var hmacData = crypt.hmac.sha1(key,data).getValue(); 
hmacData = crypt.bin.encodeBase64( hmacData );

console.log( hmacData,#hmacData )
